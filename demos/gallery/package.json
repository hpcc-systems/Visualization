        "@hpcc-js/codemirror-shim": "^2.36.0",
        "@hpcc-js/common": "^2.72.0",
        "@hpcc-js/comms": "^2.97.0",
        "@hpcc-js/composite": "^2.8.2",
        "@hpcc-js/ddl-shim": "^2.21.0",
        "@hpcc-js/deck-shim": "^2.7.0",
        "@hpcc-js/dgrid": "^2.33.0",
