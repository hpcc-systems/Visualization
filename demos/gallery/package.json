        "@hpcc-js/codemirror-shim": "^2.38.0",
        "@hpcc-js/common": "^2.73.2",
        "@hpcc-js/comms": "^2.102.1",
        "@hpcc-js/composite": "^2.9.4",
        "@hpcc-js/ddl-shim": "^2.25.0",
        "@hpcc-js/deck-shim": "^2.9.0",
