{
  "name": "@hpcc-js/chart",
  "version": "0.8.8",
  "description": "hpcc-js - Viz Chart",
  "main": "dist/index.js",
  "module": "dist/index.es6",
  "unpkg": "dist/index.min.js",
  "types": "types/index.d.ts",
  "files": [
    "dist/*",
    "types/*",
    "src/*.css"
  ],
  "scripts": {
    "clean": "rimraf lib* && rimraf types && rimraf dist",
    "compile-es6": "tsc --module es6 --outDir ./lib-es6",
    "compile-es6-watch": "npm run compile-es6 -- -w",
    "compile-umd": "tsc --module umd --outDir ./lib-umd",
    "bundle": "rollup -c",
    "bundle-watch": "npm run bundle -- -w",
    "minimize": "uglifyjs dist/index.js -c -m --source-map  -o dist/index.min.js",
    "build": "npm run compile-es6 && npm run bundle",
    "watch": "concurrently --kill-others \"npm run compile-es6-watch\" \"npm run bundle-watch\"",
    "lint": "tslint --project . src/**/*.ts",
    "docs": "typedoc --options tdoptions.json ."
  },
  "dependencies": {
    "@hpcc-js/api": "^0.4.10",
    "@hpcc-js/common": "^0.8.8",
    "@hpcc-js/util": "^0.1.7",
    "d3-array": "1.2.1",
    "d3-axis": "1.0.8",
    "d3-brush": "1.0.4",
    "d3-cloud": "1.2.5",
    "d3-color": "1.2.0",
    "d3-contour": "1.1.2",
    "d3-format": "1.3.0",
    "d3-geo": "1.6.4",
    "d3-hexbin": "0.2.2",
    "d3-hierarchy": "1.1.4",
    "d3-hsv": "0.1.0",
    "d3-interpolate": "1.1.5",
    "d3-scale": "1.0.6",
    "d3-selection": "1.1.0",
    "d3-shape": "1.2.0",
    "d3-svg-annotation": "2.2.2",
    "d3-time-format": "2.1.0",
    "d3-transition": "1.1.0",
    "d3-zoom": "1.7.1",
    "d3v4-bullet": "1.0.6",
    "tslib": "1.9.0"
  },
  "devDependencies": {
    "@hpcc-js/bundle": "^0.0.42",
    "@types/d3-shape": "1",
    "concurrently": "3.5.1",
    "rimraf": "2.6.2",
    "rollup": "0.58.2",
    "rollup-plugin-alias": "1.4.0",
    "rollup-plugin-commonjs": "9.1.0",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-postcss": "1.6.1",
    "tslint": "5.9.1",
    "typedoc": "0.7.1",
    "typescript": "2.9.1",
    "uglify-es": "3.3.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hpcc-systems/Visualization.git"
  },
  "author": "Gordon Smith <gordonjsmith@gmail.com>",
  "contributors": [],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hpcc-systems/Visualization/issues"
  },
  "homepage": "https://github.com/hpcc-systems/Visualization"
}