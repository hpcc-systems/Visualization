{
  "name": "@hpcc-js/common",
  "version": "2.5.0",
  "description": "hpcc-js - Viz Common",
  "main": "dist/index.js",
  "module": "dist/index.es6",
  "unpkg": "dist/index.min.js",
  "types": "types/index.d.ts",
  "files": [
    "dist/*",
    "types/*",
    "src/*.css",
    "font-awesome/**/*"
  ],
  "scripts": {
    "clean": "rimraf lib* types dist",
    "copy-resources": "cpx \"../../node_modules/font-awesome/fonts/*\" \"./font-awesome/fonts\" && cpx \"../../node_modules/font-awesome/css/*\" \"./font-awesome/css\"",
    "compile-es6": "tsc --module es6 --outDir ./lib-es6",
    "compile-es6-watch": "npm run compile-es6 -- -w",
    "compile-umd": "tsc --module umd --outDir ./lib-umd",
    "bundle": "rollup -c",
    "bundle-watch": "npm run bundle -- -w",
    "minimize": "uglifyjs dist/index.js -c -m --source-map  -o dist/index.min.js",
    "build": "npm run compile-es6 && npm run copy-resources && npm run bundle",
    "watch": "run-p compile-es6-watch bundle-watch",
    "lint": "tslint --project . src/**/*.ts",
    "docs": "typedoc --options tdoptions.json ."
  },
  "dependencies": {
    "@hpcc-js/util": "^2.1.2",
    "d3-selection": "1.1.0",
    "d3-transition": "1.1.0"
  },
  "devDependencies": {
    "@hpcc-js/bundle": "^0.0.45",
    "@types/d3-drag": "1.2.1",
    "@types/d3-dsv": "1",
    "@types/d3-scale": "1",
    "@types/d3-selection": "1.3.1",
    "@types/d3-transition": "1",
    "colorbrewer": "1.0.0",
    "cpx": "1.5.0",
    "d3-array": "1.2.1",
    "d3-brush": "1.0.4",
    "d3-collection": "1.0.4",
    "d3-color": "1.2.0",
    "d3-dispatch": "1.0.3",
    "d3-drag": "1.2.1",
    "d3-dsv": "1.0.8",
    "d3-ease": "1.0.3",
    "d3-format": "1.3.0",
    "d3-interpolate": "1.1.5",
    "d3-path": "1.0.5",
    "d3-scale": "1.0.6",
    "d3-time-format": "2.1.0",
    "d3-zoom": "1.7.1",
    "font-awesome": "4.7.0",
    "npm-run-all": "4.1.3",
    "rimraf": "2.6.2",
    "rollup": "0.63.5",
    "rollup-plugin-alias": "1.4.0",
    "rollup-plugin-commonjs": "9.1.4",
    "rollup-plugin-node-resolve": "3.3.0",
    "rollup-plugin-postcss": "1.6.2",
    "tslib": "1.9.3",
    "tslint": "5.11.0",
    "typedoc": "0.12.0",
    "typescript": "3.0.1",
    "uglify-js": "3.4.9"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hpcc-systems/Visualization.git"
  },
  "author": "Gordon Smith <gordonjsmith@gmail.com>",
  "contributors": [],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hpcc-systems/Visualization/issues"
  },
  "homepage": "https://github.com/hpcc-systems/Visualization"
}
