{
  "name": "@hpcc-js/ddl",
  "version": "0.0.25",
  "description": "hpcc-js DDL parser",
  "main": "lib/index.js",
  "module": "lib-es6/index.js",
  "types": "lib/index.d.ts",
  "files": [
    "lib/*",
    "lib-es6/*",
    "dist/*"
  ],
  "scripts": {
    "build": "npm run create-schema && tsc",
    "clean": "rimraf ./src/ddl.json && rimraf ./lib* && rimraf ./dist*",
    "create-schema": "echo export const ddlSchema = > ./src/ddlSchema.ts && typescript-json-schema --noExtraProps --required ./src/ddl.ts DDLSchema >> ./src/ddlSchema.ts && echo ; >> ./src/ddlSchema.ts"
  },
  "dependencies": {
    "ajv": "^4.11.3",
    "typescript-json-schema": "^0.14.0",
    "tslib": "^1.7.1"
  },
  "devDependencies": {
    "rimraf": "^2.6.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hpcc-systems/Visualization.git"
  },
  "author": "Gordon Smith <gordonjsmith@gmail.com>",
  "contributors": [],
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hpcc-systems/Visualization/issues"
  },
  "homepage": "https://github.com/hpcc-systems/Visualization"
}