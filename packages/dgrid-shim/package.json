{
  "name": "@hpcc-js/dgrid-shim",
  "version": "0.1.13",
  "description": "dgrid shim",
  "main": "dist/index.js",
  "types": "types/index.d.ts",
  "scripts": {
    "clean": "rimraf lib* || rimraf types || rimraf dist",
    "compile-es6": "tsc --module es6 --outDir ./lib-es6",
    "compile-es6-watch": "npm run compile-es6 -- -w",
    "compile-amd": "tsc --module amd --outDir ./lib-amd",
    "compile-cjs": "tsc --module commonjs --outDir ./lib-cjs",
    "compile-umd": "tsc --module umd --outDir ./lib-umd",
    "bundle": "webpack",
    "minimize": "uglifyjs dist/index.js -c -m --source-map  -o dist/index.min.js",
    "build": "npm run compile-cjs && npm run bundle",
    "lint": "tslint --project . src/**/*.ts"
  },
  "files": [
    "dist/**/*",
    "types/*"
  ],
  "dependencies": {
    "tslib": "1.9.0"
  },
  "devDependencies": {
    "@hpcc-js/bundle": "^0.0.45",
    "@types/dojo": "1.9.40",
    "copy-webpack-plugin": "4.5.2",
    "css-loader": "1.0.0",
    "dgrid": "1.2.1",
    "dijit": "1.13.0",
    "dojo": "1.13.0",
    "dojo-dstore": "1.1.2",
    "dojo-themes": "1.13.0",
    "dojo-util": "1.13.0",
    "dojo-webpack-plugin": "2.7.4",
    "dojox": "1.13.0",
    "rimraf": "2.6.2",
    "style-loader": "0.19.0",
    "tslint": "5.9.1",
    "typescript": "2.9.1",
    "uglify-es": "3.3.9",
    "uglifyjs-webpack-plugin": "1.2.7",
    "url-loader": "0.6.2",
    "webpack": "4.16.4",
    "webpack-cli": "3.0.8",
    "webpack-hasjs-plugin": "1.0.1"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hpcc-systems/Visualization.git"
  },
  "keywords": [
    "dojo",
    "webpack",
    "dgrid",
    "dstore"
  ],
  "author": "Gordon Smith",
  "license": "Apache-2.0",
  "bugs": {
    "url": "https://github.com/hpcc-systems/Visualization/issues"
  },
  "homepage": "https://github.com/hpcc-systems/Visualization#readme"
}