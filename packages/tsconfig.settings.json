{
    "compilerOptions": {
        "composite": true,
        "target": "es5",
        "module": "umd",
        "moduleResolution": "node",
        "sourceMap": true,
        "declaration": true,
        "declarationMap": true,
        "noImplicitAny": false,
        "noEmitOnError": false,
        "noUnusedLocals": true,
        "strictNullChecks": false,
        "importHelpers": true,
        "experimentalDecorators": true,
        "emitDecoratorMetadata": false,
        "lib": [
            "dom",
            "es5",
            "es2015.promise"
        ],
        "types": []
    }
}