{
  "private": true,
  "name": "@hpcc-js/test-dgrid",
  "version": "2.6.0",
  "dependencies": {
    "@hpcc-js/common": "^2.16.2",
    "@hpcc-js/dgrid": "^2.4.2",
    "@hpcc-js/dgrid-shim": "^2.6.0",
    "@hpcc-js/test-data": "^2.0.10",
    "chai": "4.2.0",
    "es6-promise": "4.2.6",
    "mocha": "5.2.0",
    "simulate-event": "1.4.0"
  },
  "devDependencies": {
    "@types/chai": "4.1.6",
    "@types/mocha": "5.2.5",
    "cpx": "1.5.0",
    "css-loader": "2.1.1",
    "file-loader": "1.1.5",
    "mocha-chrome": "1.1.0",
    "rimraf": "2.6.3",
    "source-map-loader": "0.2.1",
    "style-loader": "0.23.1",
    "typescript": "3.3.3",
    "webpack": "4.29.6",
    "webpack-cli": "3.2.3"
  },
  "scripts": {
    "clean": "rimraf lib* types dist",
    "copy-resources": "cpx ./node_modules/@hpcc-js/dgrid-shim/dist/dojo/resources/* ./dist/dojo/resources",
    "compile-es6": "tsc --module es6 --outDir ./lib-es6",
    "compile-es6-watch": "npm run compile-es6 -- -w",
    "bundle": "webpack",
    "bundle-watch": "npm run bundle -- -w",
    "watch": "npm run compile-es6-watch && npm run bundle-watch",
    "build": "npm run compile-es6 && npm run bundle && npm run copy-resources",
    "test": "mocha-chrome ./test.html"
  }
}
