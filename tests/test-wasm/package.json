{
  "private": true,
  "name": "@hpcc-js/test-wasm",
  "version": "2.0.0",
  "dependencies": {
    "@hpcc-js/wasm": "^2.0.0",
    "chai": "4.2.0",
    "es6-promise": "4.2.6",
    "mocha": "5.2.0",
    "simulate-event": "1.4.0"
  },
  "devDependencies": {
    "@types/chai": "4.1.6",
    "@types/mocha": "5.2.5",
    "cpx": "1.5.0",
    "css-loader": "2.1.1",
    "file-loader": "1.1.5",
    "mocha-chrome": "1.1.0",
    "npm-run-all": "4.1.5",
    "rimraf": "2.6.3",
    "source-map-loader": "0.2.1",
    "style-loader": "0.23.1",
    "typescript": "3.4.1",
    "webpack": "4.29.6",
    "webpack-cli": "3.2.3"
  },
  "scripts": {
    "clean": "rimraf lib* types dist *.tsbuildinfo",
    "copy-resources": "cpx ./node_modules/@hpcc-js/wasm/dist/graphvizlib.wasm ./dist",
    "compile-es6": "tsc --module es6 --outDir ./lib-es6",
    "compile-es6-watch": "npm run compile-es6 -- -w",
    "bundle": "npm run copy-resources && webpack",
    "bundle-watch": "npm run bundle -- -w",
    "watch": "run-p compile-es6-watch bundle-watch",
    "build": "npm run compile-es6 && npm run bundle",
    "test": "mocha-chrome http://127.0.0.1:8080/tests/test-wasm/test.html",
    "dev-start": "cd .. && cd .. && npm run dev-start"
  }
}